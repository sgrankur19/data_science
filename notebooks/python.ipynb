   "source": [
    "words = [\"bat\", \"tab\", \"tap\", \"pat\", \"cat\", \"act\", \"tac\"]\n",
    "\n",
    "groups = {}\n",
    "for word in words:\n",
    "    key = ''.join(sorted(word))\n",
    "    if key not in groups:\n",
    "        groups[key] = []\n",
    "    groups[key].append(word)"
   ]
